<div class="row ml-3">
    <div class="col-3">
        <h4>List of TV Shows</h4>
        <div class="list-group list-group-flush">
            {{#each tvShows}}
            <a href="{{../root}}/tvshow/{{this.name}}" class="list-group-item list-group-item-action">
                {{this.name}}
            </a>
            {{/each}}
        </div>
    </div>
</div>